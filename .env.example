# Banco de Dados (PostgreSQL)
DB_HOST=postgres
DB_PORT=5432
DB_NAME=atenbot
DB_USER=postgres
DB_PASSWORD=sua_senha_aqui

# Google Gemini AI
GEMINI_API_KEY=sua_chave_principal_aqui
GEMINI_API_KEY_BACKUP=sua_chave_reserva_aqui
GEMINI_MODEL=gemini-2.5-flash
GEMINI_MAX_TOKENS=8000
GEMINI_TEMPERATURE=0.7

# Evolution API (WhatsApp)
EVOLUTION_API_URL=http://evolution-api:8080
EVOLUTION_API_KEY=sua_evolution_api_key
EVOLUTION_INSTANCE=atenbot
# Habilita segurança no webhook (opcional, true/false)
WEBHOOK_SECURITY_ENABLED=true

# Redis (BullMQ Queue)
REDIS_HOST=redis
REDIS_PORT=6379

# Configurações Gerais
NODE_ENV=development
PORT=3000
